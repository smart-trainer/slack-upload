{
  "name": "slack-upload",
  "version": "1.0.0",
  "description": "Github action for uploading files to slack",
  "main": "lib/index.js",
  "scripts": {
    "build-tsc": "tsc",
    "build-ncc": "ncc build",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "build": "run-s build-tsc build-ncc format format-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smart-trainer/slack-upload.git"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@slack/web-api": "^5.15.0"
  },
  "devDependencies": {
    "@types/node": "^14.11.5",
    "@vercel/ncc": "^0.24.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "resolutions": {
    "minimist": "^1.2.2"
  }
}
